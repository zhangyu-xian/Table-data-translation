<Table3.0.1>
    <Table 表名="表 3.0.1水利水电工程分等指标">
        <水利水电工程 工程等别="Ⅰ">
            <工程规模>大(1)型</工程规模>
            <水库总库容>≥10（10^8m^3）</水库总库容>
			<保护人口>≥150（10^4人）</防洪>
			<保护农田面积>≥500（10^4亩）</保护农田面积>
            <保护区当量经济规模>≥300（10^4人）</保护区当量经济规模>
            <治涝面积>≥200（10^4亩）</治涝面积>
            <灌溉面积>≥150（10^4亩）</灌溉面积>
			<供水对象重要性>特别重要</供水对象重要性>
            <年引水量>≥10（10^8m^3）</年引水量>
            <发电装机容量>≥1200（MW）</发电装机容量>
        </水利水电工程>
        <水利水电工程 工程等别="Ⅱ">
            <工程规模>大(2)型</工程规模>
            <水库总库容>&lt;10（10^8m^3）,≥1.0（10^8m^3）</水库总库容>
			<保护人口>&lt;150（10^4人）,≥50（10^4人）</防洪>
			<保护农田面积>&lt;500（10^4亩）,≥100（10^4亩）</保护农田面积>
            <保护区当量经济规模>&lt;300（10^4人）,≥100（10^4人）</保护区当量经济规模>
            <治涝面积>&lt;200（10^4亩）,≥60（10^4亩）</治涝面积>
            <灌溉面积>,&lt;150（10^4亩）,≥50（10^4亩）</灌溉面积>
			<供水对象重要性>重要</供水对象重要性>
            <年引水量>&lt;10（10^8m^3）,≥3（10^8m^3）</年引水量>
            <发电装机容量>&lt;1200（MW）,≥300（MW）</发电装机容量>
        </水利水电工程>
        <水利水电工程 工程等别="Ⅲ">
            <工程规模>中型</工程规模>
            <水库总库容>&lt;1.0（10^8m^3）,≥0.10（10^8m^3）</水库总库容>
			<保护人口>&lt;50（10^4人）,≥20（10^4人）</防洪>
			<保护农田面积>&lt;100（10^4亩）,≥30（10^4亩）</保护农田面积>
            <保护区当量经济规模>&lt;100（10^4人）,≥40（10^4人）</保护区当量经济规模>
            <治涝面积>&lt;60（10^4亩）,≥15（10^4亩）</治涝面积>
            <灌溉面积>,&lt;50（10^4亩）,≥5（10^4亩）</灌溉面积>
			<供水对象重要性>比较重要</供水对象重要性>
            <年引水量>&lt;3（10^8m^3）,≥1（10^8m^3）</年引水量>
            <发电装机容量>&lt;300（MW）,≥50（MW）</发电装机容量>
        </水利水电工程>
        <水利水电工程 工程等别="Ⅳ">
            <工程规模>小(1)型</工程规模>
            <水库总库容>&lt;0.1（10^8m^3）,≥0.01（10^8m^3）</水库总库容>
			<保护人口>&lt;20（10^4人）,≥5（10^4人）</防洪>
			<保护农田面积>&lt;30（10^4亩）,≥5（10^4亩）</保护农田面积>
            <保护区当量经济规模>&lt;40（10^4人）,≥10（10^4人）</保护区当量经济规模>
            <治涝面积>&lt;15（10^4亩）,≥3（10^4亩）</治涝面积>
            <灌溉面积>,&lt;5（10^4亩）,≥0.5（10^4亩）</灌溉面积>
			<供水对象重要性>一般</供水对象重要性>
            <年引水量>&lt;1（10^8m^3）,≥0.3（10^8m^3）</年引水量>
            <发电装机容量>&lt;50（MW）,≥10（MW）</发电装机容量>
        </水利水电工程>
        <水利水电工程 工程等别="V">
            <工程规模>小(2)型</工程规模>
            <水库总库容>&lt;0.01（10^8m^3）,≥0.001（10^8m^3）</水库总库容>
			<保护人口>&lt;5（10^4人）</防洪>
			<保护农田面积>&lt;5（10^4亩）</保护农田面积>
            <保护区当量经济规模>&lt;10（10^4人）</保护区当量经济规模>
            <治涝面积>&lt;3（10^4亩）</治涝面积>
            <灌溉面积>,&lt;0.5（10^4亩）</灌溉面积>
			<供水对象重要性>一般</供水对象重要性>
            <年引水量>&lt;0.3（10^8m^3）</年引水量>
            <发电装机容量>&lt;10（MW）</发电装机容量>
        </水利水电工程>
    </Table>
